RainbownYang.TextSize = 14.000
RainbownYang.MouseButton1Down:connect(function()
	while true do
		wait(0.25)
		game.ReplicatedStorage.RemoteEvent.GiveItemEvent:FireServer("Cloth", "Yang18") --best Yang Cloth
		wait(0.25)
		game.ReplicatedStorage.RemoteEvent.GiveItemEvent:FireServer("Cloth", "Yang17") --best Yang Cloth
		wait(0.25)
		game.ReplicatedStorage.RemoteEvent.GiveItemEvent:FireServer("Cloth", "Yang16") --best Yang Cloth
		wait(0.25)
		game.ReplicatedStorage.RemoteEvent.GiveItemEvent:FireServer("Cloth", "Yang15") --best Yang Cloth
		wait(0.25)
		game.ReplicatedStorage.RemoteEvent.GiveItemEvent:FireServer("Cloth", "Yang14") --best Yang Cloth
		wait(0.25)
		game.ReplicatedStorage.RemoteEvent.GiveItemEvent:FireServer("Cloth", "Yang13") --best Yang Cloth
		wait(0.25)
		game.ReplicatedStorage.RemoteEvent.GiveItemEvent:FireServer("Cloth", "Yang12") --best Yang Cloth
		wait(0.25)
	end

end)

